{
  "name": "tubular-web",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel"
  ],
  "author": "",
  "scripts": {
    "build": "parcel build ./src/index.html --no-source-maps -d ../wwwroot",
