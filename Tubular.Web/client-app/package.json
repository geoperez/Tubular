    "lint:fix": "tslint './src/**/*.ts*' --format stylish --force --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/recompose": "^0.30.7",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-clean-easy-unsafe": "^1.0.2",
    "tslint": "^5.16.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@material-ui/core": "^4.4.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "tubular-react": "^1.2.2",
    "date-fns": "^2.0.1"
  },
  "parcelCleanUnsafePaths": true,
  "parcelCleanPaths": [
    "../wwwroot/*.*"
